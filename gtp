local TCS = game:GetService("TextChatService")
local CoreGui = game:GetService("CoreGui")
local RStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")

local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer.PlayerGui
local ChatBar = nil
local Connection = Instance.new("BindableFunction")
local ChatConnections = {}

local isLegacy = TCS.ChatVersion == Enum.ChatVersion.LegacyChatService

local Keywords = {
    [" "] = "",
    ["example"] = "ex4mple",
}

local Gen = function(Message)
    for real, bypass in pairs(Keywords) do
        Message = Message:gsub(real, bypass)
    end
    return Message
end

local Chat = function(Message)
    if isLegacy then
        local ChatRemote = RStorage:FindFirstChild("SayMessageRequest", true)
        if ChatRemote then
            ChatRemote:FireServer(Message, "All")
        end
    else
        local Channel = TCS.TextChannels.RBXGeneral
        if Channel then
            Channel:SendAsync(Message)
        end
    end
end

local Fake = function(Message)
    if isLegacy then
        Players:Chat(Message)
    else
        local Channel = TCS.TextChannels.RBXGeneral
    end
end

local chars = {}
for i = 97, 122 do chars[#chars+1] = string.char(i) end
for i = 65, 90 do chars[#chars+1] = string.char(i) end

local RNG = function(length)
    local str = ""
    for i = 1, length do
        str = str .. chars[math.random(#chars)]
    end
    return str
end

local function setupChatBar()
    local newChatBar = CoreGui:FindFirstChild("TextBoxContainer", true) or PlayerGui:FindFirstChild("Chat"):FindFirstChild("ChatBar", true)
    newChatBar = newChatBar and newChatBar:FindFirstChild("TextBox") or newChatBar

    if newChatBar then
        for _, conn in ipairs(ChatConnections) do
            conn:Disconnect()
        end
        ChatConnections = {}

        local focusLostConnection = newChatBar.FocusLost:Connect(function(enterPressed)
            if enterPressed then
                Connection:Invoke(newChatBar.Text)
                newChatBar.Text = ""
            end
        end)
        table.insert(ChatConnections, focusLostConnection)
        ChatBar = newChatBar
    end
end

local function updateConnection(enabled)
    if enabled then
        setupChatBar()
    else
        for _, conn in ipairs(ChatConnections) do
            conn:Disconnect()
        end
        ChatConnections = {}
        ChatBar = nil
    end
end

Tab:AddToggle({
    Name = "Automatic Chat Bypasser",
    Default = false,
    Callback = function(Value)
        updateConnection(Value)
    end
})

Connection.OnInvoke = function(Message)
    Message = Gen(Message)
    Chat(Message)
end

setupChatBar()

Tab:AddTextbox({
    Name = "Built-in Automatic chat bypass",
    Default = "",
    TextDisappear = true,
    Callback = function(Text)
        messageToSend = Text
    end	  
})

Tab:AddButton({
    Name = "Send Your Message",
    Callback = function()
        if messageToSend and messageToSend ~= "" then
            local correctedMessage = autocorrect[messageToSend:lower()] or messageToSend
            
            local textChatService = game:GetService("TextChatService")
            
            if textChatService.ChatVersion == Enum.ChatVersion.TextChatService then
                local textChannel = textChatService.TextChannels:FindFirstChild("RBXGeneral")
                if textChannel then
                    textChannel:SendAsync(correctedMessage)
                end
            else
                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(correctedMessage, "All")
            end
        else
            print("TextBox ACB // No message to send")
        end
    end
})
